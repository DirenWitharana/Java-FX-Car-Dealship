<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="700.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.cw_fx.AdminHomePageController">
   <children>
      <Label fx:id="welcomeAdmin" layoutX="394.0" layoutY="14.0" text="Welcome Admin">
         <font>
            <Font size="29.0" />
         </font>
      </Label>
      <Button fx:id="addAndEditCar" layoutX="405.0" layoutY="150.0" mnemonicParsing="false" onAction="#addCarPage" prefHeight="40.0" prefWidth="191.0" text="Add and Edit car">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="buyer" layoutX="405.0" layoutY="231.0" mnemonicParsing="false" onAction="#buyRequests" prefHeight="39.0" prefWidth="191.0" text="Buyer Offers">
         <font>
            <Font size="19.0" />
         </font>
      </Button>
      <Button fx:id="testDrive" layoutX="405.0" layoutY="310.0" mnemonicParsing="false" onAction="#testDriveRequests" prefHeight="40.0" prefWidth="191.0" text="Test Drive Requests">
         <font>
            <Font size="19.0" />
         </font>
      </Button>
      <Button fx:id="back" layoutX="14.0" layoutY="635.0" mnemonicParsing="false" onAction="#back" text="Back" />
   </children>
</AnchorPane>
