<AnchorPane prefHeight="700.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.cw_fx.CustomerPageController">
   <children>
      <Label fx:id="welcomeCustomer" layoutX="364.0" layoutY="30.0" prefHeight="46.0" prefWidth="273.0" text="Welcome Customer">
         <font>
            <Font size="31.0" />
         </font>
      </Label>
      <Button fx:id="carSale" layoutX="411.0" layoutY="182.0" mnemonicParsing="false" onAction="#carSaleIn" prefHeight="44.0" prefWidth="179.0" text="Car Sale">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Button fx:id="carService" layoutX="411.0" layoutY="284.0" mnemonicParsing="false" onAction="#serviceIn" prefHeight="44.0" prefWidth="179.0" text="Car Service">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Button layoutX="14.0" layoutY="636.0" mnemonicParsing="false" onAction="#back" text="Back" />
   </children>
</AnchorPane>
