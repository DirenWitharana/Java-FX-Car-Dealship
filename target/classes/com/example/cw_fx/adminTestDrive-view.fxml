<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="testDriveRequests" prefHeight="700.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.cw_fx.AdminTestDriveController">
   <children>
      <Label layoutX="375.0" layoutY="14.0" text="Test Drive Requests">
         <font>
            <Font size="29.0" />
         </font>
      </Label>
      <TableView fx:id="tableView" layoutX="3.0" layoutY="133.0" prefHeight="306.0" prefWidth="994.0">
        <columns>
          <TableColumn fx:id="colCustomerName" prefWidth="97.0" text="Cus Name" />
          <TableColumn fx:id="colAddress" prefWidth="123.0" text="Address" />
            <TableColumn fx:id="colPhoneNo" prefWidth="88.0" text="Phone No" />
            <TableColumn fx:id="colMake" prefWidth="78.0" text="Make" />
            <TableColumn fx:id="colModel" prefWidth="70.0" text="Model" />
            <TableColumn fx:id="colYear" prefWidth="82.0" text="Year" />
            <TableColumn fx:id="colMileage" prefWidth="85.0" text="Mileage" />
            <TableColumn fx:id="colOldOwner" prefWidth="98.0" text="Old Owner" />
            <TableColumn fx:id="colSellPrice" minWidth="0.0" prefWidth="95.0" text="Sell Price" />
            <TableColumn fx:id="colAvailability" prefWidth="83.0" text="Availability" />
            <TableColumn fx:id="colStatus" prefWidth="94.0" text="Status" />
        </columns>
      </TableView>
      <Button layoutX="770.0" layoutY="495.0" mnemonicParsing="false" onAction="#addAppointment" text="Add Appointment">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <DatePicker fx:id="date" layoutX="732.0" layoutY="449.0" prefHeight="25.0" prefWidth="216.0" />
      <Button layoutX="14.0" layoutY="625.0" mnemonicParsing="false" onAction="#back" text="Back" />
   </children>
</AnchorPane>
